{
  "name": "shrikeh/diving",
  "description": "Barney's personal website about diving and an opportunity to learn things",
  "license": "proprietary",
  "require-dev": {
    "ext-xdebug": "*",
    "behat/behat": "^3.5@dev",
    "friends-of-behat/symfony-extension": "^2.1@dev",
    "infection/infection": "^0.15.0",
    "overtrue/phplint": "^1.2",
    "phpmetrics/phpmetrics": "^2.5",
    "phpspec/phpspec": "^6.1",
    "phpstan/phpstan": "^0.12.8",
    "phpunit/phpunit": "^8.5",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/thanks": "^1.2"
  },
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "psr/http-message": "^1.0@dev",
    "symfony/console": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.0.*",
    "symfony/yaml": "5.0.*",
    "webonyx/graphql-php": "^0.13.8"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "application/app",
      "Shrikeh\\Diving\\": "application/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Behat\\": "tests/behat/src",
      "Tests\\App\\": "tests/unit/app"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  },
  "minimum-stability": "dev"
}
