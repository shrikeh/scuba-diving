{
  "name": "shrikeh/diving",
  "description": "Barney's personal website about diving and an opportunity to learn things",
  "license": "proprietary",
  "require-dev": {
    "ext-xdebug": "*",
    "behat/behat": "^3.5@dev",
    "codeception/codeception": "4.0.x-dev",
    "codeception/module-asserts": "^1.0.0",
    "codeception/module-symfony": "dev-master",
    "friends-of-behat/symfony-extension": "^2.1@dev",
    "friends-of-phpspec/phpspec-code-coverage": "^4.0@dev",
    "infection/infection": "^0.15.0",
    "jangregor/phpstan-prophecy": "dev-master",
    "overtrue/phplint": "^1.2",
    "php-ds/php-ds": "dev-master",
    "phpmd/phpmd": "dev-master",
    "phpmetrics/phpmetrics": "dev-master",
    "phpspec/phpspec": "^6.1",
    "phpstan/extension-installer": "dev-master",
    "phpstan/phpstan": "^0.12.0@dev",
    "phpstan/phpstan-phpunit": "^0.12.0@dev",
    "phpunit/phpunit": "^8.5",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/thanks": "^1.2",
    "webonyx/graphql-php": "dev-master"
  },
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "guzzlehttp/psr7": "^1.6",
    "nyholm/psr7": "^1.0@dev",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0@dev",
    "psr/http-message": "^1.0@dev",
    "psr/simple-cache": "^1.0@dev",
    "ramsey/uuid": "^4.0@dev",
    "symfony/cache": "5.0.*",
    "symfony/console": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.0.*",
    "symfony/http-client": "5.0.*",
    "symfony/messenger": "5.0.*",
    "symfony/yaml": "5.0.*"
  },
  "config": {
    "preferred-install": {
        "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "application/app",
      "Shrikeh\\Diving\\": "application/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Behat\\": "tests/behat/src",
      "Tests\\Codecept\\": "tests/integration/support",
      "Tests\\Unit\\App\\": "tests/unit/app",
      "spec\\Shrikeh\\Diving\\": "tests/spec"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
      "auto-scripts": {
          "cache:clear": "symfony-cmd",
          "assets:install %PUBLIC_DIR%": "symfony-cmd"
      }
  },
  "conflict": {
      "symfony/symfony": "*"
  },
  "extra": {
      "symfony": {
        "allow-contrib": true,
        "require": "5.0.*",
        "config-dir": "application/config",
        "src-dir": "application/app",
        "public-dir": "application/public"
      }
  },
  "minimum-stability": "dev"
}
