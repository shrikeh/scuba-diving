{
  "timeout": 10,
  "source": {
    "directories": [
      "application/app",
      "application/src"
    ],
    "excludes": [
      "Kernel"
    ]
  },
  "phpUnit": {
    "configDir": "."
  },
  "logs": {
    "text": "build/logs/infection/general.log",
    "summary": "build/logs/infection/summary.log",
    "debug": "build/logs/infection/debug.log",
    "badge": {
        "branch": "master"
    }
  },
  "tmpDir": "build/cache",
  "mutators": {
    "@default": true
  }
}
